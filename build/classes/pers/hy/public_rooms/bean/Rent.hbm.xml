<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
       "-//Hibernate/Hibernate Mapping DTD3.0//EN"
       "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
       
<hibernate-mapping>  
    <class name="pers.hy.public_rooms.bean.Rent" table="rent">  
        <id name="roomId" column="ROOM_ID">  
            <generator class="assigned"/>  
        </id>   
        <property name="roomName" column="ROOM_NAME"/>  
        <property name="renterName" column="RENTER_NAME"/>
        <property name="renterId" column="RENTER_ID"/>   
        <property name="renterPhone" column="RENTER_PHONE"/> 
        <property name="rentStartDate" column="RENT_START_DATE"/>
        <property name="rentEndDate" column="RENT_END_DATE"/> 
        <property name="rentHire" column="RENT_HIRE"/>    
        <property name="rentOther" column="RENT_OTHER"/>
    </class>  
</hibernate-mapping>  