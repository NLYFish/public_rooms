<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
       "-//Hibernate/Hibernate Mapping DTD3.0//EN"
       "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
       
<hibernate-mapping>  
    <class name="pers.hy.public_rooms.bean.Room" table="room">  
        <id name="roomId" column="ROOM_ID">  
            <generator class="assigned"/>  
        </id>   
        <property name="roomName" column="ROOM_NAME"/>   
        <property name="roomBuilding" column="ROOM_BUILDING"/> 
        <property name="roomFloor" column="ROOM_FLOOR"/> 
        <property name="roomArea" column="ROOM_AREA"/> 
        <property name="roomHire" column="ROOM_HIRE"/>
        <property name="roomOther" column="ROOM_OTHER"/> 
    </class>  
</hibernate-mapping> 