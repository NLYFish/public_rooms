<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
       "-//Hibernate/Hibernate Mapping DTD3.0//EN"
       "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
       
<hibernate-mapping>  
    <class name="pers.hy.public_rooms.bean.User" table="user">  
        <id name="userId" column="USER_ID">  
            <generator class="assigned"/>  
        </id>   
        <property name="userNo" column="USER_NO"/> 
        <property name="userName" column="USER_NAME"/> 
        <property name="userPhone" column="USER_PHONE"/> 
        <property name="userPassword" column="USER_PASSWORD"/>   
    </class>  
</hibernate-mapping>  